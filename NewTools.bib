   pmid = {20110278},
   publisher = {Oxford University Press},
   title = {BEDTools: a flexible suite of utilities for comparing genomic features},
   volume = {26},
   url = {/pmc/articles/PMC2832824/ /pmc/articles/PMC2832824/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2832824/},
