   pmid = {23104886},
   publisher = {Oxford University Press},
   title = {STAR: ultrafast universal RNA-seq aligner},
   volume = {29},
   url = {/pmc/articles/PMC3530905/ /pmc/articles/PMC3530905/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3530905/},
   year = {2013},
}
